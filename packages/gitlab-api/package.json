{
  "name": "gitlab-api",
  "version": "0.0.1",
  "license": "MIT",
  "private": false,
  "main": "./build/index.js",
  "types": "./typings/index.d.ts",
  "scripts": {
    "update:patch": "npm version patch",
    "update:minor": "npm version minor",
    "update:major": "npm version major",
    "message": "echo \"Building old version $npm_package_version into new version...\"",
    "build": "npm run message && tsc --build --force",
    "build:patch": "npm run build && npm run update:patch",
    "build:minor": "npm run build && npm run update:minor",
    "build:major": "npm run build && npm run update:major",
    "test": "mocha"
  },
  "engines": {
    "node": ">=v14.18.1"
  },
  "exports": "./build/index.js",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.23",
    "chai": "^4.3.6",
    "mocha": "^9.2.2",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2"
  },
  "files": [
    "build",
    "typings"
  ]
}
